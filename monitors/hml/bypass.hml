with
    proc_lib:init_p(_,_,_,_,[_,_,_,{local, 'Elixir.Graft.Supervisor'},_,_,_])
monitor
    and([_Launcher <- _Server, proc_lib:init_p(_,_,_,_,_)] 
        max(X.
            and(
                [_Server -> _, proc_lib:init_p(_,_,_,_,_)]X,
                [_Server <- _Node, proc_lib:init_p(_,_,_,_,_)]X,
                [_Server ? _]X,
                [_Server ! _]X,
                [_Node ** _]X
            )
        )
    ).